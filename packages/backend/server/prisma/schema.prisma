generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("SQLITE_URL")
}

//多维表格应用
model BiApp {
  id     String    @id @default(uuid())
  name   String?    
  tables BiTable[]

  @@map("bi_app")
}

model BiTable {
  id      String @id @default(uuid())
  name    String? 
  biApp   BiApp  @relation(fields: [biAppId], references: [id])
  biAppId String

  @@map("bi_table")
}
